{
    "name": "litgroup/enumerable",
    "description": "Implementation of the enumerable type for PHP.",
    "keywords": ["enumerable", "enum"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Shamritskiy",
            "email": "roman@litgroup.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "LitGroup\\Enumerable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\LitGroup\\Enumerable\\": "test"
        }
    },
    "archive": {
        "exclude": [
            "/tests",
            "tests.sh",
            "/phpunit.xml.dist",
            "/.travis.yml",
            ".gitignore"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require": {
        "php": "~5.5|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    }
}
